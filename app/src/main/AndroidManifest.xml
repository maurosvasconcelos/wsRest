<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mauro.wsrest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/teste_icon"
        android:label="@string/app_name"
        android:logo="@mipmap/teste_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.projeto.Activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.projeto.Activity.CadastrarPessoa"
            android:label="@string/title_activity_cadastrar_pessoa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.projeto.Activity.Main2Activity"></activity>
    </application>

</manifest>